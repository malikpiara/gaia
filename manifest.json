{
    "name": "Gaia",
    "description": "Hello World extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"    
      },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Save Link to Gaia"
    }
}